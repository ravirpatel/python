# Create an empty dictionary called dog
dog = {}

# Add name, color, breed, legs, age to the dog dictionary
dog['name'] = 'ravi'
dog['color'] = 'Brown'
dog['breed'] = 'Golden Retriever'
dog['legs'] = 4
dog['age'] = 3

# Create a student dictionary
student = {
    'first_name': 'ravo',
    'last_name': 'patel',
    'gender': 'Male',
    'age': 21,
    'marital_status': 'Single',
    'skills': ['Python', 'JavaScript'],
    'country': 'india',
    'city': 'gujarat',
    'address': 'abc'
}

# Get the length of the student dictionary
student_length = len(student)

# Get the value of skills and check the data type, it should be a list
skills_value = student['skills']
skills_data_type = type(skills_value)

# Modify the skills values by adding one or two skills
student['skills'].extend(['HTML', 'CSS'])

# Get the dictionary keys as a list
keys_list = list(student.keys())

# Get the dictionary values as a list
values_list = list(student.values())

# Change the dictionary to a list of tuples using items() method
student_tuples = list(student.items())

# Delete one of the items in the dictionary
del student['marital_status']

# Delete one of the dictionaries
del student

# Print the results
print("Dog Dictionary:", dog)
print("Student Dictionary:", student)  # This line will raise an error since student was deleted
print("Length of Student Dictionary:", student_length)
print("Skills Value:", skills_value)
print("Skills Data Type:", skills_data_type)
print("Modified Student Dictionary:", student)
print("Keys List:", keys_list)
print("Values List:", values_list)
print("Student Tuples:", student_tuples)
